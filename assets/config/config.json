{
	/*
	*	u_		Up
	*	ur		Up Right
	*	ul		Up Left
	*	l_		Left
	*	d_		Down
	*	dr		Down Right
	*	dl		Down Left
	*	r_		Right
	*	BACK	Go back
	*	EXIT	Close launcher
	*/
	"keys": {
		"u_": ["k", "Up", "8", "w"],
		"ur": ["u", "9"],
		"ul": ["y", "7"],
		"l_": ["h", "Left", "4", "a"],
		"d_": ["j", "Down", "2", "s"],
		"dr": ["n", "3"],
		"dl": ["b", "1"],
		"r_": ["l", "Right", "6", "d"],
		"BACK": ["Backspace"],
		"EXIT": ["Escape"]
	},
	
	// The following hotkey is an XLib keysym
	"hotkey": "Super_L",

	/*
	*	Mask        | Value | Key
	*	------------+-------+------------
	*	ShiftMask   |     1 | Shift
	*	LockMask    |     2 | Caps Lock
	*	ControlMask |     4 | Ctrl
	*	Mod1Mask    |     8 | Alt
	*	Mod2Mask    |    16 | Num Lock
	*	Mod3Mask    |    32 | Scroll Lock
	*	Mod4Mask    |    64 | Windows
	*/
	"hotkey_modifier": 0,

	// Color scheme used by the application in
	// RGBA format
	"colors": {
		"unselected": [255, 255, 255, 100],
		"selected": [255, 223, 0, 100],
		"highlighted": [30, 144, 255, 100],
		"line": [30, 200, 0, 200]
	},

	// If true, animated sprites are rendered
	"render_sprites": false
}
